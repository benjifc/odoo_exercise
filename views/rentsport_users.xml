<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="rentsport_users_tree" model="ir.ui.view">
        <field name="name">res.partner.tree</field>
        <field name="model">res.partner</field>
        <field name="arch" type="xml">
            <tree string="Usuarios" create="False">
                <field name="name"/>
                <field name="phone"/>
                <field name="email"/>
            </tree>
        </field>
    </record>
    <record id="rentsport_users_action" model="ir.actions.act_window">
        <field name="name">Usuarios</field>
        <field name="res_model">res.partner</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree</field>
        <field name="domain">[('es_usuario','=','True')]</field>
        <field name="context">{'default_es_usuario': 'True'}</field>
    </record>
</odoo>